= f.fields_for @address do |a|
  .form-group
    = a.label :street, 'Улица'
    = a.text_field :street, class: 'form-control'
  .form-group
    = a.label :house, 'Дом'
    = a.text_field :house, class: 'form-control'
  .form-group
    = a.label :flat, 'Квартира'
    = a.text_field :flat, class: 'form-control'


